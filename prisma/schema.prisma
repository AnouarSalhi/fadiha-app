generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model prank {
  id           String @id
  title        String
  videoUrl     String
  thumbnailUrl String
  visits       Int    @default(0)
}

model user {
  id       String @id
  username String @unique(map: "User_username_key")
  prankId  String
  visits   Int    @default(0)
}
